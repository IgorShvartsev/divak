{
   "name": "igorshvartsev/divak",
   "description": "Divak - basic, minimalistic PHP framework",
   "version": "1.1.2",
   "autoload": {
      "classmap": [
         "core"
      ],
      "psr-4": {
         "Library\\": "app/library",
         "Helpers\\": "app/helpers",
         "Middleware\\": "app/middleware"
      }
   },
   "require": {
      "guzzlehttp/guzzle": "~6.0",
      "intervention/image": "^2.3",
      "verot/class.upload.php": "dev-master",
      "pimple/pimple"  : "~3.0",
      "monolog/monolog": "^1.25",
      "mashape/unirest-php": "^3.0",
      "nesbot/carbon": "^2.25"
   },
   "config": {
      "vendor-dir": "vendor",
      "process-timeout": 1800,
      "sort-packages": true
   }
}
